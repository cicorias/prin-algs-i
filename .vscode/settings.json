{   
    "java.trace.server":"verbose",
    "java.configuration.checkProjectSettingsExclusions": false,
    "java.checkstyle.version": "8.35",
    "java.checkstyle.configuration": "https://scicoria.blob.core.windows.net/public/google_checks_local.xml",
    "java.checkstyle.properties": {
        "basedir": "${workspaceFolder}",
        "suppressions" : "${workspaceFolder}/lib/checkstyle-suppressions.xml"
    },
    "java.checkstyle.modules": [
        "${workspaceFolder}/lib/checkstyle-8.31/checkstyle-8.31-all.jar",
        "${workspaceFolder}/lib/checkstyle-8.31/checkstyle-lift.jar"
    ]
}